#include <stdio.h>

int main(int argc, char **argv)
{
	int a,b;
	scanf("%d %d",&a,&b);
	int a100,a10,a1,b100,b10,b1;
	int ra,rb;
	a100=(a/100);
	a10=(a%100)/10;
	a1=a%10;
	b100=(b/100);
	b10=(b%100)/10;
	b1=b%10;
	
	ra=a1*100 + a10*10 + a100;
	rb=b1*100 + b10*10 + b100;
	
	if(ra>rb){
		printf("%d\n",ra);
	}else{
		printf("%d\n",rb);
	}
	return 0;
}
