#include <stdio.h>

int main(int argc, char **argv)
{
	int x,y;
	scanf("%d",&x);
	scanf("%d",&y);
	if(x!=0 && y!=0){
		if(x<0){
			if(y<0){
				printf("%d\n",3);
			}else{
				printf("%d\n",2);
			}
		}else{
			if(y<0){
				printf("%d\n",4);
			}else{
				printf("%d\n",1);
			}
		}
	}else{
		return 0;
	}
	return 0;
}
